{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args":
    {
    	"type": "flat"
    },
    "replay_buffer_size": 100000,
    "algorithm_kwargs": {
        "render_agent_pos": false,
        "log_episode_alphas": false,
        "num_epochs":5000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":1000,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
    	{"soft_reset_wrapper": {"max_time": 500}},
    	{"rendering_observation": 
			{
				"resize": [64,64,1]
			}},
        {"vae_wrapper": {
            "vae": null,
            "channel_last": true,
            "image_channels": 3,
            "latent_size": 32,
            "conv_net": "VAE2",
            "steps": 200,
            "steps_per_train_delay": 1000,
            "step_skip": 10,
            "perform_pretraining": 5000,
            "dtype": "uint8",
            "hist_size": 2500,
            "learning_rate": 0.0005,
            "kl_term": 1.0
        }},
        { "smirl_wrapper": {
            "buffer_type": "Gaussian",
            "minimize": false
        }},
        {"add_text_info_to_rendering":  {
            "log_returns": true
        }}
    ],
    "log_comet": false,
    "env": "GDY-BiomesPartiallyObservedPixel-v0",
    "env_kwargs": {
    	"reward_func": null,
        "max_steps": 500,
        "episode_length": 500
    }
}
