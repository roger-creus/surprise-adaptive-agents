{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args":
    {
    	"type": "flat"
    },
    "replay_buffer_size": 1000000,
    "algorithm_kwargs": {
        "render_agent_pos": true,
        "num_epochs":3000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":1000,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
        {"add_alpha": true},
    	{"soft_reset_wrapper": {"max_time": 500}},
        {"resize_observation_wrapper": {
                "new_size": [7,7,3],
                "new_shape": [7,7,3],
                "grayscale": false
        }},

        {"rescale_rgb":  true},

        {"rendering_observation":
		{
			"resize": [118,40,1],
            "render_agent_obs": true,
            "resize_agent_obs": [56, 56, 3],
            "rescale_agent_obs": 255
		}},
        { "surprise_adapt_wrapper": {
    		"buffer_type": "Gaussian",
            "surprise_window_len" : -1
        }}
    ],
    "log_comet": false,
    "env": "MiniGrid-SurpriseAdaptRooms-v0",
    "env_kwargs": {
        "num_doors": 3

    }
}
