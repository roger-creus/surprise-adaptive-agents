{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args": 
    {
    	"type": "flat"
    },
    "replay_buffer_size": 100000,
    "algorithm_kwargs": {
        "render": false,
        "render_agent_pos": false,
        "log_episode_alphas": false,
        "num_epochs":5000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":1000,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": false
    },
    "wrappers": [
        {"add_alpha" : true},
    	{"soft_reset_wrapper": {"max_time": 250}},
    	{"rendering_observation": 
			{
				"resize": [64,48,1]
			}},
        { "surprise_adapt_bandit_wrapper": {
                "buffer_type": "Gaussian"
            }},
        {"add_text_info_to_rendering":  {
            "log_returns": true
        }}

    ],
    "log_comet": false,
    "env": "MazeEnvFullyObserved",
    "snapshot_mode": "none",
    "env_kwargs": {
    	"reward_func": null,
        "max_steps": 250,
        "episode_length": 250
    }
}
