{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args": 
    {
    	"type": "flat"
    },
    "replay_buffer_size": 1000000,
    "algorithm_kwargs": {
        "log_episode_alphas" : true,
        "num_epochs":6000,
        "num_eval_steps_per_epoch":200,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":200,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
        {"add_alpha" : true},
    	{"soft_reset_wrapper": {"max_time": 100}},
    	{"rendering_observation": 
			{
				"resize": [64,48,1]
			}},
        { "surprise_adapt_wrapper": {
            "buffer_type": "Bernoulli",
            "surprise_window_len": 10
        }}
    ],
    "log_comet": false,
    "env": "Tetris",
    "env_kwargs": {
    	"reward_func": null,
        "max_steps": 100
    }
}