{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
	"online": true,
    "network_args": 
    {
    	"type": "conv_mixed",
    	"size": 1809
    },
    "replay_buffer_size": 1000000,
    "algorithm_kwargs": {
        "num_epochs":5000,
        "num_eval_steps_per_epoch":1000,
		"num_train_loops_per_epoch": 1000,
        "num_trains_per_train_loop":1,
		"num_expl_steps_per_train_loop":10,
        "min_num_steps_before_training":5000,
        "max_path_length":5000,
        "batch_size":512
    },
	"exploration_kwargs": {
        "prob_random_action": 1,
        "prob_end": 0.03,
        "steps": 500000
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
    	{"soft_reset_wrapper": {"max_time": 1000}},
    	{"dict_obs_wrapper": {
    		"obs_key": "observation"
    		}
    	},
    	{"resize_observation_wrapper": {
	        "new_size": [26,20,1],
	        "new_shape": [20,26,1],
	        "grayscale": true,
	        "obs_key": "observation",
	        "out_key_name": "small_img"
	        }},
    	{"resize_observation_wrapper": {
	        "new_size": [64,48,1],
	        "new_shape": [48,64,1],
	        "grayscale": true,
	        "obs_key": "observation"
	        }},
    	{"rendering_observation": 
			{
				"resize": [256,256,1]
			}},
    	{"ObsHistoryWrapper": {
    		"history_length": 4,
    		"stack_channels": true,
    		"channel_dim": 2,
    		"obs_key": "observation"
    	}},
    	{"smirl_wrapper": {
    		"buffer_type": "Gaussian",
    		"obs_label": "small_img",
    		"obs_out_label": "theta",
    		"smirl_rew_scale": 1,
    		"latent_obs_size": 520,
    		"add_true_rew": false,
			"minimize": false
    	}},
    	{"dict_to_obs_wrapper": {
    		"obs_keys": ["observation", "theta"],
            "obs_size": 13329
    	}},
        {"crafter_wrapper": {
    		"save_metrics": true,
            "save_metrics_path": "metrics"
    	}}
    ],
    "log_comet": true,
    "env": "Crafter",
    "env_kwargs": {
        "max_steps": 1000
    }
}