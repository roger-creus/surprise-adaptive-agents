{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args": 
    {
    	"type": "conv_mixed",
    	"size": 13578
    },
    "replay_buffer_size": 25000,
    "algorithm_kwargs": {
        "num_epochs":3000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":50,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
    	{"soft_reset_wrapper": {"max_time": 1000}},
    	{"resize_observation_wrapper": {
	        "new_size": [64,48,1],
	        "new_shape": [48,64,1],
	        "grayscale": true
	        }},
    	{"rendering_observation": 
			{
				"resize": [256,256,1]
			}},
    	{"ObsHistoryWrapper": {
    		"history_length": 4,
    		"stack_channels": true,
    		"channel_dim": 2
    	}},
    	{ "surprise_adapt_bandit_wrapper": {
			"buffer_type": "Gaussian"
		}},
        {"crafter_wrapper": {
    		"save_metrics": true,
            "save_metrics_path": "metrics"
    	}}
    ],
    "log_comet": true,
    "env": "Crafter",
    "env_kwargs": {
        "max_steps": 50
    }
}