{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args":
    {
    	"type": "cnn",
        "n_channels": [16, 32, 64],
        "kernel_sizes": [2,2,2],
        "strides": [1,1,1],
        "paddings": [0,0,0]
    },
    "replay_buffer_size": 1000000,
    "algorithm_kwargs": {
        "num_epochs":3000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":1000,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
    	{"soft_reset_wrapper": {"max_time": 500}},
        {"resize_observation_wrapper": {
	        "new_size": [7,7,3],
	        "new_shape": [7,7,3],
	        "grayscale": false
        }},
        {"rescale_rgb":  true},
    	{"rendering_observation":
		{
			"resize": [118,40,1]
		}},
        { "smirl_wrapper": {
    		"buffer_type": "Gaussian"
        }}
    ],
    "log_comet": false,
    "env": "MiniGrid-SurpriseAdaptRooms-v0",
    "env_kwargs": {
    }
}