{
    "algorithm":"SAC",
    "version":"normal",
    "layer_size":256,
    "network_args":
    {
    	"type": "flat"
    },
    "replay_buffer_size": 100000,
    "algorithm_kwargs": {
        "episode_length":500,
        "num_epochs":3000,
        "num_eval_steps_per_epoch":1000,
        "num_trains_per_train_loop":1000,
        "num_expl_steps_per_train_loop":1000,
        "min_num_steps_before_training":1000,
        "max_path_length":1000,
        "batch_size":256
    },
    "trainer_kwargs": {
        "discount":0.99,
        "learning_rate":3E-4
    },
    "render_kwargs":{
        "render": true
    },
    "wrappers": [
    	{"soft_reset_wrapper": {"max_time": 500}},
        {"resize_observation_wrapper": {
	        "new_size": [9,9,3],
	        "new_shape": [9,9,3],
	        "grayscale": false
        }},
        {"rescale_rgb":  true},

    	{"rendering_observation":
		{
			"resize": [118,40,1],
            "render_agent_obs": true,
            "resize_agent_obs": [24, 24, 3],
            "rescale_agent_obs": 255
		}},
        {"FlattenObservationWrapper":  {}},
    	{"dict_obs_wrapper": {
    		"obs_key": "observation"
    		}
    	},
    	{"ICMWrapper": {
    		"stuff": true,
    		"encoding_size": 64,
    		"network_type": "flat",
    		"obs_key": "observation",
    		"network": null,
    		"hist_size": 100000
    		}
    		},
		{ "smirl_wrapper": {
    		"buffer_type": "Gaussian",
    		"add_true_rew": "only",
    		"obs_label": "observation",
    		"obs_out_label": "theta"
        }},
    	{"dict_to_obs_wrapper": {
    		"obs_keys": ["observation"],
    		"obs_size": 243
    	}}
    ],
    "log_comet": false,
    "env": "MiniGrid-SurpriseAdaptRooms-v0",
    "env_kwargs": {
        "num_doors": 3,
        "agent_view_size": 3,
        "noisy_room": 1
    }
}